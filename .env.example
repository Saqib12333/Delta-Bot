# Environment selection: demo or live
DELTA_ENV=demo

# Demo and Live trade URLs (can be overridden if needed)
DELTA_DEMO_URL=https://demo.delta.exchange/app/futures/trade/BTC/BTCUSD
DELTA_LIVE_URL=https://www.delta.exchange/app/futures/trade/BTC/BTCUSD

# Optional: explicit URL override (takes precedence if set)
# DELTA_TRADE_URL=

# CDP port for Edge DevTools Protocol
CDP_PORT=9222

# Allow the bot to auto-close Edge and relaunch with CDP when needed (0/1)
EDGE_ALLOW_KILL=0
